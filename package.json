{

  "name": "react-bootstrap-fontawesome",
  "version": "0.5.18",
  "description": "A TS framework for React, Bootstrap & FontAwesome.",

  "type": "module",
  "typings": "./lib/dist/main.d.ts",
  "main": "./lib/dist/main.js",

  "exports": {
    ".": {
      "require": {
        "types": "./lib/dist/main.d.ts",
        "default": "./lib/dist/main.js"
      }
    }
  },

  "scripts": {

    "prepare": "npx husky || true",

    "lint-back": "npx eslint --config .eslintrc-back.js --ext .ts,.cts,.mts ./lib/src/**/*",
    "lint-tests": "npx eslint --config .eslintrc-tests.js --ext .js,.cjs,.mjs ./test/**/*",
    "lint": "npm run lint-back && npm run lint-tests",

    "clean-back": "npx rimraf ./lib/dist",
    "clean": "npm run clean-back",

    "build-back": "npm run clean-back && npx tsc --project ./tsconfig.json",
    "build": "npm run build-back",
    "build-tests": "npx webpack --config \"./webpack.config-test.js\"",

    "check-requires": "npx used-deps-analyzer ./package.json ./lib/src --no-dev",
    "check-updates": "npx check-version-modules",
    "unit-tests": "npm run build && npm run build-tests && npx nyc --reporter=html --reporter=text mocha",

    "tests": "npm run lint && npm run check-requires && npm run check-updates && npm run unit-tests"

  },

  "files": [
    "/lib/dist"
  ],
  "engines": {
    "node": ">=22.22.0"
  },

  "dependencies": {},
  "devDependencies": {
    "@types/react": "19.2.13",
    "@types/react-dom": "19.2.3",
    "check-version-modules": "2.2.0",
    "colors": "1.4.0",
    "eslint-plugin-personnallinter": "git+ssh://git@github.com/Psychopoulet/eslint-plugin-personnallinter",
    "husky": "9.1.7",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "ts-loader": "9.5.4",
    "used-deps-analyzer": "0.1.8",
    "webpack": "5.105.1",
    "webpack-cli": "6.0.1"
  },

  "keywords": [
    "typescript",
    "react",
    "bootstrap",
    "typescript"
  ],

  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run unit-tests"
    }
  },

  "author": "SÃ©bastien VIDAL",
  "license": "ISC",

  "homepage": "https://github.com/Psychopoulet/react-bootstrap-fontawesome#readme",
  "repository": {
    "type": "git",
    "url": "git://github.com/Psychopoulet/react-bootstrap-fontawesome.git"
  },
  "bugs": {
    "url": "https://github.com/Psychopoulet/react-bootstrap-fontawesome/issues"
  }

}
